{
    "php.validate.executablePath": "C:\\PHP7\\php.exe",
    "telemetry.enableTelemetry": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "workbench.colorTheme": "Cobalt2",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.wordWrap": "on",
    "editor.minimap.side": "left",
    "editor.acceptSuggestionOnEnter": "on",
    "editor.fontLigatures": true,
    "breadcrumbs.enabled": true,
    "editor.tabSize": 2,
    "editor.fontFamily": "FiraCode, Hack,  'Courier New', monospace",
    // Controls the delay in ms after which quick suggestions will show up.
    "editor.quickSuggestionsDelay": 10,

    // Controls if suggestions should automatically show up when typing trigger characters
    "editor.suggestOnTriggerCharacters": true,

    // Controls if pressing tab inserts the best suggestion and if tab cycles through other suggestions
    "editor.tabCompletion": "on",

    // Controls whether sorting favours words that appear close to the cursor
    "editor.suggest.localityBonus": true,

    // Controls how suggestions are pre-selected when showing the suggest list
    "editor.suggestSelection": "first",

    // Enable word based suggestions
    "editor.wordBasedSuggestions": true,

    // Enable parameter hints
    "editor.parameterHints.enabled": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "php": "html",
        "js": "html"
    },
    
    "emmet.syntaxProfiles": {
        "javascript": "jsx",
        "php": "html",
        "vue-html": "html"
    },
    "explorer.confirmDragAndDrop": false,

    "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
    "python.disableInstallationCheck": true,
    "python.linting.enabled": true,
    "files.associations": {
        "*.html": "html",
        "*.py": "python",
        "*.js": "javascript",
        "*.php": "php",
    },
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "jsx",
        "vue",
        "vue-html"
    ],
    "python.jediEnabled": true,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "files.autoSave": "afterDelay",
    "[php]": {
    
},
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "php.validate.run": "onType",
    "editor.defaultFormatter": "vscode.emmet",
    "editor.formatOnSaveTimeout": 0,
    "editor.formatOnType": true,
    "git.enableSmartCommit": true,
    "css.completion.triggerPropertyValueCompletion": true,
    "less.validate": true,
    "scss.validate": true,
    "css.lint.argumentsInColorFunction": "warning",
    "css.lint.duplicateProperties": "warning",
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.boxModel": "warning",
    "css.lint.universalSelector": "warning",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "php-cs-fixer.executablePath": "${extensionPath}\\php-cs-fixer.phar",
    "php-cs-fixer.lastDownload": 1563040500819,
    // File extensions to be parsed by the Typescript parser
    "importCost.typescriptExtensions": [
        "\\.tsx?$",
        "\\.jsx?$",
        "\\.vue?$",
    ],
    "workbench.startupEditor": "newUntitledFile",
    "[markdown]": {
        "editor.quickSuggestions": true
    },
    "emmet.triggerExpansionOnTab": true, // enable tab to expanse emmet tags
    "blade.format.enable": true,         // if you would like to enable blade format
}